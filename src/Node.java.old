public class Node {
    int value;
    int id;
    Node next;

    public Node() {
        this.next = null;
    }

    public Node(int value) {
        this.value = value;
    }

    public Node(int value, int id) {
        this.value = value;
        this.id = id;
    }

    public Node(int value, int id, Node next) {
        this.value = value;
        this.id = id;
        this.next = next;
    }

    public int getValue() {
        return value;
    }

    public int getId() {
        return id;
    }

    public Node getNext() {
        return next;
    }

    public void setNext(Node next) {
        this.next = next;
    }

    public void setId(int id) {
        this.id = id;
    }

    public void setValue(int value) {
        this.value = value;
    }
}
